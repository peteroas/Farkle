<h1>Show Game</h1>

<ul>

  <li>
    <strong>Code:</strong>
    <%= @game.code %>
  </li>

  <li>
    <strong>Winning score:</strong>
    <%= @game.winning_score %>
  </li>

  <li>
    <strong>Punishment:</strong>
    <%= @game.punishment %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.game_path(@conn, :edit, @game) %></span>
<span><%= link "Back", to: Routes.game_path(@conn, :index) %></span>
